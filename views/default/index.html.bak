{{extend 'layout.html'}}


<h2>
Previous Posts
</h2>
<table class="table">
    {{for row in rows:}}
    <tr>
        <td><a href="{{=URL('show',args = row.id)}}">{{=row.title}}</a></td>
        <td style="width: 200px">{{=row.time_stamp}}</td>
        <td style="width: 600px">{{=row.body}}</td>
        <td style="width: 200px">{{=row.sold}}</td>
        <td>
        <img class="item" ,="" width="200px" src="{{=URL('download', args=row.file)}}" />
        </td>
    </tr>
    {{pass}}
</table>
